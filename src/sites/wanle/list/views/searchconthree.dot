{{var destCityList = proAll[0].DcList;}}
{{var themeList = proAll[0].ThemeList;}}
{{var timeList = proAll[0].PmList;}}
{{var languageList = proAll[0].SingleLanguageList;}}

<div class="searchConBox">
    <div class="searchConBox_each searchConBox_each_3" data-conboxeachindex="3">
        <div class="searchConList">
            {{? destCityList!="" && destCityList.length > 0}}
	        <dl class="search_dl" data-key="dcId">
	            <dt class="search_dt">目的城市</dt>
	            <dd class="search_dd setParentDirect">
	                <a data-id="0" class="search_all on" trace="nolimit_city">不限</a>
	                {{? destCityList!="" && destCityList.length > 0}}
	                <ul class="search_ul dest_ul">
	                    {{~ destCityList:descCity:dindex}}
	                    {{? dindex > 6}}
	                    <li class="none"><a data-id="{{=descCity.Id}}" trace="descCity_{{=dindex+1}}">{{=descCity.Name}}</a></li>
	                    {{??}}
	                    <li><a data-id="{{=descCity.Id}}" trace="descCity_{{=dindex+1}}">{{=descCity.Name}}</a></li>
	                    {{?}}
	                    {{~}}
	                </ul>
	                {{?}}
	                {{? destCityList!="" && destCityList.length > 7}}
	                <div class="open_all">
	                    <span href="javascript:void(0);" class="more_even">
	                        <em>更多</em><i></i>
	                    </span>
	                </div>
	                {{?}}
	            </dd>
	        </dl>
	        {{?}}

	        {{? tabList !="" && tabList.length >0}}
            <dl class="search_dl" data-key="SingleType">
                <dt class="search_dt">游玩主题</dt>
                <dd class="search_dd setParentDirect">
                    <a data-id="0" class="search_all on" trace="nolimit_playtheme">不限</a>
                    {{? tabList !="" && tabList.length >0}}
                        <ul class="search_ul">
                            {{~ tabList:theme:themeIndex}}
                            {{? theme.Id != 6 && theme.Id != 9}}
                                <li><a data-id="{{=theme.Id}}" trace="playtheme_{{=themeIndex+1}}">{{=theme.Name}}</a></li>
                            {{?}}
                            {{~}}
                        </ul>
                    {{?}}
                </dd>
            </dl>
            {{?}}

            {{? themeList!="" && themeList.length > 0 }}
                <dl class="search_dl" data-key="themeId" data-flag="1">
                    <dt class="search_dt">游玩分类</dt>
                    <dd class="search_dd MultiSelect">
                        <a data-id="0" class="search_all on" trace="nolimit_play">不限</a>
                        <ul class="search_ul dest_ul">
                            {{~ themeList:palytheme:tindex}}
                            {{? tindex > 6}}
                            <li class="none"><a data-id="{{=palytheme.Id}}" trace="palytheme_{{=tindex+1}}"><i></i>{{=palytheme.Name}}</a></li>
                            {{??}}
                            <li><a data-id="{{=palytheme.Id}}" trace="palytheme_{{=tindex+1}}"><i></i>{{=palytheme.Name}}</a></li>
                            {{?}}
                            {{~}}
                        </ul>
                        {{? themeList!="" && themeList.length > 7 }}
                        <div class="open_all">
                            <span href="javascript:void(0);" class="more_even">
                                <em>更多</em><i></i>
                            </span>
                        </div>
                        {{?}}
                    </dd>
                </dl>
            {{?}}

            {{? timeList!="" && timeList.length > 0}}
             <dl class="search_dl" data-key="pm">
                <dt class="search_dt">游玩时间</dt>
                <dd class="search_dd setParentDirect">
                    <a data-id="0" class="search_all on" trace="nolimit_time">不限</a>
                    {{? timeList!="" && timeList.length > 0}}
                    <ul class="search_ul">
                        {{~ timeList:timeItem:mindex}}
                        {{? mindex <= 5}}
                        <li><a data-id="{{=timeItem.YearMonth}}" trace="time_{{=mindex+1}}">{{=timeItem.Month}}月</a></li>
                        {{?}}
                        {{~}}
                    </ul>
                    {{?}}
                </dd>
            </dl>
            {{?}}

            {{? languageList !="" && languageList.length > 0}}
            <dl class="search_dl" data-key="singleLanguage">
                <dt class="search_dt">服务语言</dt>
                <dd class="search_dd setParentDirect">
                    <a data-id="0" class="search_all on" trace="nolimit_language">不限</a>
                    {{? languageList !="" && languageList.length > 0}}
                    <ul class="search_ul dest_ul">
                        {{~ languageList:language:lanIndex}}
                            {{? lanIndex > 6}}
                                <li class="none"><a data-id="{{=language.Id}}" trace="language_{{=index+1}}">{{=language.Name}}</a></li>
                            {{??}}
                                <li><a data-id="{{=language.Id}}" trace="language_{{=index+1}}">{{=language.Name}}</a></li>
                            {{?}}
                        {{~}}
                    </ul>
                    {{?}}
                    {{? languageList !="" && languageList.length > 6}}
                    <div class="open_all">
                        <span href="javascript:void(0);" class="more_even">
                            <em>更多</em><i></i>
                        </span>
                    </div>
                    {{?}}
                </dd>
            </dl>
            {{?}}
            <dl class="search_dl" data-key="MinPrice,MaxPrice">
                <dt class="search_dt">价格范围</dt>
                <dd class="search_dd setParentDirect">
                    <a data-id="," class="search_all on">不限</a>
                    <ul class="search_ul">
                        <li><a data-id="0,200" trace="price_1">￥200以下</a></li>
                        <li><a data-id="200,500" trace="price_2">￥200-￥500</a></li>
                        <li><a data-id="500,800" trace="price_3">￥500-￥800</a></li>
                        <li><a data-id="800," trace="price_4">￥800以上</a></li>
                    </ul>

                    <div class="sort_price">
                        <label class="label-price">
                            价格区间
                            <input type="text" maxlength="8" class="input-price input-left">
                            <i>¥</i>
                        </label>
                        <i>~</i>
                        <label class="label-price label-price1">
                            <input type="text" maxlength="8" class="input-price input-right">
                            <i>¥</i>
                        </label>
                        <span class="price-btn J_trace" trace="price_region">确定</span>
                    </div>
                </dd>
            </dl>
            <dl class="search_dl" data-key="minSingleTime,maxSingleTime" >
                <dt class="search_dt">活动时长</dt>
                <dd class="search_dd setParentDirect">
                    <a data-id="," class="search_all on">不限</a>
                    <ul class="search_ul">
                        <li><a data-id="0,3">0~3小时</a></li>
                        <li><a data-id="3,5">3~5小时</a></li>
                        <li><a data-id="5,7">5~7小时</a></li>
                        <li><a data-id="7,24">7小时~1天</a></li>
                        <li><a data-id="48,">2天以上</a></li>
                    </ul>
                </dd>
            </dl>

        </div>
        <div class="searchTotalBox clearfix">
            <span class="total">为您找到了<em class="pro_total">{{=totalCount}}</em>份产品</span>
            <div class="crumbox">
                <span class="removeall none">清除</span>
            </div>
        </div>

        <div class="searchSortBox">
            <div class="SortBox_left">
                <a class="searchSortBox_a active" data-role="recommend" trace="tc_sort_recommend">
                    <em>同程推荐</em>
                </a>
                <a class="searchSortBox_a" data-role="price" data-sort="1" trace="tc_sort_price">
                    <em>价格<i></i></em>
                </a>
                <a class="searchSortBox_a other_a" data-role="hot" data-sort="1" trace="tc_sort_use">
                    <em>销量<i></i></em>
                </a>
                <a class="searchSortBox_a other_a" data-role="rate" data-sort="1" trace="tc_sort_satis">
                    <em style="border-right: none">满意度<i></i></em>
                </a>
            </div>
            <div class="SortBox_right">
                <div class="sortKey" data-key="reserve">
                    <a class="SortBox_add" data-id="1" trace="tc_sort_today"><span><i></i>可订今日</span></a>
                </div>
                <div class="sortKey" data-key="dischannel">
                    <a class="SortBox_add" data-id="1" trace="tc_sort_coupon"><span><i></i>可享优惠</span></a>
                </div>
            </div>
        </div>
    </div>
</div>

