{{var navList = data.HotDestNavList;}}
{{var subCityArrays=[
    {City:["香港","澳门","台湾"],Url: ["xianggang","aomen","taiwan"]},
    {City:["东京","大阪"],Url: ["dongjing","daban"]},
    {City:["新加坡","曼谷","清迈","普吉岛"],Url: ["xinjiapo","mangu","qingmai","pujidao"]},
    {City:["美国","英国","法国","意大利"],Url: ["meiguo","yingguo","faguo","yidali"]},
    {City:["迪拜","悉尼","墨尔本","新西兰"],Url: ["dibai","xini","moerben","xinxilan"]},
    {City:["巴厘岛","沙巴","毛里求斯"],Url: ["balidao","shaba","maoliqiusi"]},
];}}

<ul class="naviga-box" trackkit="Category_destination">
{{? navList !="" && navList.length > 0}}
    {{var subCitys = "";}}
    {{~ navList:destList:index}}
	    {{? destList != ""}}
	        <li data-index="{{=index+1}}">
		        <div class="tab-item">
	                <em class="tab{{=index+1}}"></em>
	                <div class="tab-box">
	                    <p class="main-city">
	                        {{? destList.DestList.length > 0}}
	                            {{~ destList.DestList:dest}}
	                            <span>{{=dest}}</span>
	                            {{~}}
	                        {{?}}
	                    </p>
	                    <p class="sub-city">
						{{~subCityArrays[index].Url:sunCityUrl:cIndex}}
							<a href="{{?index == 5}}/dujia/wanle/searchlist_{{=sunCityUrl}}.html{{??}}/dujia/wanle/{{=sunCityUrl}}-menpiao/{{?}}" target="_blank" trace="{{=subCityArrays[index].City[cIndex]}}" trackspot="{{=subCityArrays[index].City[cIndex]}}^外显^{{=index+1}}^0">{{=subCityArrays[index].City[cIndex]}}</a>
						{{~}}
	                    </p>
	                </div>
	                <i></i>
	            </div>
	            {{? destList.AreaList.length > 0}}
                    {{var showDouble = "";}}
                        {{? destList.AreaList.length  == 2}}
                        {{showDouble = "show-double";}}
                        {{?}}
                        <div class="naviga-box-content clearfix {{=showDouble}} none" data-index="{{=index+1}}">
                        {{~ destList.AreaList:cityDetail:cityIndex}}
							<div class="naviga-module">
                                <h3>{{=cityDetail.AreaName}}</h3>
								{{? cityDetail.HotDestinationList.length > 0}}
                                 <dl>
                                    <dt>热门目的地</dt>
                                    <dd>
                                        {{~ cityDetail.HotDestinationList:hotDest:pageOne}}
                                        <a href="{{?hotDest.Url && hotDest.Url.length}}{{=hotDest.Url}}{{??}}/dujia/wanle/searchlist_{{=hotDest.CityName}}.html{{?}}" target="_blank"  trace="new{{=hotDest.CityName}}" onclick = "_tcTraObj._tcTrackEvent('search', '/lineWanle/menpiao/homepage', '/sbox/k/hot', '|*|k:{{=hotDest.CityName}}|*|pos:{{=pageOne+1}}|*|jpTp:1|*|pgLoc:sideNavigation|*|');" trackspot="{{=hotDest.CityName}}^hover^{{=index+1}}^热门目的地">{{=hotDest.CityName}}</a>
                                        {{~}}
                                    </dd>
                                </dl>
                                {{?}}
                                {{? cityDetail.HotProductList.length > 0}}
                                 <dl>
                                    <dt>特色玩法</dt>
                                    <dd>
                                        {{~ cityDetail.HotProductList:hotProduct}}
                                        <a href="/dujia/wanle/{{=hotProduct.ProductId}}.html"  trace="{{=hotProduct.MainTitle}}" target="_blank" class="{{=hotProduct.Color}}" trackspot="{{=hotProduct.MainTitle}}^hover^{{=index+1}}^特色玩法">{{=hotProduct.MainTitle}}</a>
                                        {{~}}
                                    </dd>
                                </dl>
                                {{?}}
                            </div>
                        {{~}}
                        </div>
                {{?}}
            </li>
        {{?}}
    {{~}}
{{?}}
</ul>