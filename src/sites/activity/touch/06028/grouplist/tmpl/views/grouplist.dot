
{{~it.Data:list:index}}
  <div class="item">
    <a href="/dujia/zhuanti/myaccount.html?groupNo={{=list.GroupId}}&statue={{=list.CheckStatus}}">
        <h3><label><i>กรุ๊ปโค๊ด</i><br/>团号：</label>{{=list.GroupNo}}<span><label><i>No.</i><br/>团次：</label>{{=list.Number}}</span></h3>
        <div class="item-info">
		   <div class="right">
					<div>
						<p class="tai">
						  {{=list.CostCount}}บันทึกการเบิกเงิน
						</p>
						<p>	
						  {{=list.CostCount}}条报账记录
						</p>					
					</div>				
					<div>
					     <p class="tai">
						    {{?list.CheckStatus==0}}
							<em>รอการตรวจสอบ</em>
							{{??}}
							<em>ตรวจแล้</em>
							{{?}}
						  </p>
						 <p>
							{{?list.CheckStatus==0}}
							<em>未审核</em>
							{{??}}
							<em>已审核</em>
							{{?}}
						  </p>						  
					</div>
					<i class="arrow arrow-right" href="javascript:void(0)"></i>
            </div>
            <div class="left">
                <div>
                    <div class="mr">
					   <p class="tai">จำนวนคน</p>
					   <p><label>人数：</label>{{=list.TouristNumber}}人</p>
					</div>
                    <div class="leader">
						<p class="tai">หัวหน้าทัวร์</p>
						<p><label>领队：</label>{{=list.LeaderName}}</p>
					</div>
                </div>
                <div>
                    <div>
						<p class="tai">ระยะเวลาทัวร</p>
						<p><label>团期：</label>{{=list.DepartDate}} ~{{=list.ReturnDate}}</p>
					</div>
                </div>
            </div>          
        </div>
    </a>
  </div>
{{~}}