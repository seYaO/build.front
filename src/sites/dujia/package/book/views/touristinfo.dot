    <div class="listbox traveller-list">
        <label class="txt">我们当中</label>
        <div>
        <input type="checkbox" value="1" id="pregnantWoman">
        <label for="pregnantWoman">有孕妇</label>
        </div>
        <div>
        <input type="checkbox" value="2" id="smalBaby">
        <label for="smalBaby">有2岁以下婴儿</label>
        </div>
        <div>
        <input type="checkbox" value="3" id="olderMan">
        <label for="olderMan">有80周岁（含）以上的老人</label>
