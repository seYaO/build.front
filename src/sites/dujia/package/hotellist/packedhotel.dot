{{?it.Hotel && it.Hotel.length>0}}
    {{~it.Hotel:hotelInfo:index}}
    <div class="traffic-hotel clearfix">
      {{?hotelInfo.HotelImage && hotelInfo.HotelImage[0] && hotelInfo.HotelImage[0].length}}
      <img class="hotel-img" src="{{=hotelInfo.HotelImage[0].replace('http:','')}}" />
      {{??}}
      <img class="hotel-img" src="//pic3.40017.cn/line/admin/2015/07/01/15/SbORAc.png" />
      {{?}}
      <div class="hotel-info">
        <p class="hotel-title">
          <a class="J_hotelTitle hotelName">{{=hotelInfo.HotelName}}</a>
         {{?hotelInfo.HotelStar && hotelInfo.HotelStarNum && hotelInfo.HotelStarNum>0}}
          <span class='level J_Starlevel' data-info='{{=hotelInfo.HotelStar}}'>
                {{for(var i = 0; i<Math.ceil(hotelInfo.HotelStarNum);i++){}}
                   {{?i <Math.floor(hotelInfo.HotelStarNum) }}
                   <em class="diamond"></em>
                   {{??}}
                   <em></em>
                   {{?}}
               {{}}}
               </span>
           {{?}}
          <span class="btn-hotel modify btn J_modify-pop"  hotel-key="" data-pop="2" data-hotelPrice="" hotel-index="">更换酒店</span>
        </p>
        {{?hotelInfo.HotelAddress}}
                       <p class="hotel-address">地址：{{=hotelInfo.HotelAddress}}</p>
                  {{?}}
                  <div class="contain-icon">
                    {{~hotelInfo.Service:service}}
                    <i class="{{= 'icon-'+service.ServeMark}}" title="{{=service.ServeName}}"></i>
                    {{~}}
                  </div>
                  <p class="date-type">
                    {{?(parseInt(hotelInfo.CheckDate.split("T")[0]) > 2000)}}
                        入住时间：<em>
                            {{=hotelInfo.CheckDate.split("T")[0]}}至{{=hotelInfo.LeaveDate.split("T")[0]}}
                            {{?hotelInfo.LiveNights>0}}
                                （{{=hotelInfo.LiveNights}}晚）
                            {{?}}
                        </em>
                    {{?}}
                    {{?hotelInfo.RoomType}}房型：<em>{{=hotelInfo.RoomType}}</em>{{?}}
                    {{?hotelInfo.RoomNum>0}}
                       （{{=hotelInfo.RoomNum}}间）
                    {{?}}
                  </p>
      </div>
    </div>
    <div class="pop popHotel">
      <div class="content">
        <div class="hotel-detail">
          <div class="img-text">
            <div class="mSlider mSlider-pop img">
              <div class="mSlider_img">
                {{?hotelInfo.HotelImage && hotelInfo.HotelImage[0] && hotelInfo.HotelImage[0].length}}
                <img id="focusPic" alt="" src="{{=hotelInfo.HotelImage[0].replace('http:','')}}">
                  {{??}}
                  <img id="focusPic" alt="" src="//pic3.40017.cn/line/admin/2015/07/01/15/SbORAc.png">
              {{?}}
            </div>
              <div class="pro_msli_pop clearfix">
                <a href="javascript:void(0);" rel="nofollow" class="group_left" id="">
                  <span class="button-prev"></span>
                </a>
                <div class="pro_msli_bd">
                  <ul class="clearfix">
                    {{~hotelInfo.HotelImage:hotelImg}}
                    <li class="">
                      {{?hotelImg && hotelImg.length}}
                      <a class="linka mleft" _bsrc="{{=hotelImg}}" href="javascript:void(0);" title="" nsrc="{{=hotelImg}}" >
                        <img src="{{=hotelImg.replace('http:','')}}" alt="">
                    </a>
                      {{??}}
                      <a class="linka mleft" _bsrc="//pic3.40017.cn/line/admin/2015/07/01/15/SbORAc.png" href="javascript:void(0);" title="" nsrc="//pic3.40017.cn/line/admin/2015/07/01/15/SbORAc.png" >
                        <img src="//pic3.40017.cn/line/admin/2015/07/01/15/SbORAc.png" alt="">
                    </a>
                      {{?}}
                    </li>
                    {{~}}
                  </ul>
                </div>
                <a rel="nofollow" href="javascript:void(0);" class="group_right">
                  <span class="button-next"></span>
                </a>
              </div>
            </div>
            <div class="text">
              <h4 class='hotelName'>{{=hotelInfo.HotelName}}</h4>
              {{?hotelInfo.HotelStar && hotelInfo.HotelStarNum && hotelInfo.HotelStarNum>0}}
                        <span class='level J_Starlevel' data-info='{{=hotelInfo.HotelStar}}'>
                              {{for(var i = 0; i<Math.ceil(hotelInfo.HotelStarNum);i++){}}
                                 {{?i <Math.floor(hotelInfo.HotelStarNum) }}
                                 <em class="diamond"></em>
                                 {{??}}
                                 <em></em>
                                 {{?}}
                             {{}}}
                             </span>
                         {{?}}
              <div class="icons">
                <p>
                  {{?hotelInfo.Service.length}}
                  {{~hotelInfo.Service:service}}<i class="{{='icon-' + service.ServeMark}}" title="{{=service.ServeName}}"></i>{{~}}
                  {{?}}
                </p>
              </div>
              <p class="address">地址：{{=hotelInfo.HotelAddress}}</p>
			  {{?hotelInfo.HotelAddress}}
			    <div class="desc">
                <p>描述：{{=hotelInfo.HotelDescribe}}</p>
              </div>
			  {{?}}
            </div>
          </div>
        </div>
      </div>
    </div>
    {{~}}
{{?}}

