{{?it.TourGuideDays&&it.TourGuideDays.length}}
    <div class="route-box routeDays-box">
        <ul>
        {{~it.TourGuideDays:itDays:index}}
            <li class="travel-day" id="travelDay{{=itDays.Day}}" data-txt="第{{=itDays.Day}}天">
                <div class="route-date-box route-title">
                    <h2>{{=itDays.Theme}}<i>D{{=itDays.Day}}</i></h2>
                </div>
                {{?itDays.Scenics&&itDays.Scenics.length}}
                {{~itDays.Scenics:StItems:index1}}
                {{?StItems.Type==1}}
                <div class="route-travel-box">
                {{?StItems.Traffic=="plane"}}
                    <div class="route-title title-arrvel">
                        <h2>{{=StItems.Destination}}<i></i></h2>
                    </div>
                {{??StItems.Traffic=="metro"}}
                    <div class="route-title bytrain">
                        <h2>{{=StItems.Destination}}<i></i></h2>
                    </div>
                {{??StItems.Traffic=="ship"}}
                    <div class="route-title byboot">
                        <h2>{{=StItems.Destination}}<i></i></h2>
                    </div>
                {{??StItems.Traffic=="car"}}
                    <div class="route-title bytaxi">
                        <h2>{{=StItems.Destination}}<i></i></h2>
                    </div>
                {{??StItems.Traffic=="walk"}}
                    <div class="route-title byfoot">
                        <h2>{{=StItems.Destination}}<i></i></h2>
                    </div>
                {{?}}
                    <div class="detail-routes">{{=StItems.Description}}
                    </div>
                </div>
                {{??StItems.Type==2}}
                    <div class="route-view-box">
                    {{?StItems.ScenicType == "other"}}
                    <div class="route-title title-view title-others">
                        <h2>其他：{{=StItems.ScenicName}}<i></i></h2>
                    </div>
                    {{??StItems.ScenicType == "scenery"}}
                    <div class="route-title title-view">
                        <h2>景点：{{=StItems.ScenicName}}<i></i></h2>
                    </div>
                    {{??StItems.ScenicType == "shopping"}}
                    <div class="route-title title-view title-shopping">
                        <h2>购物点：{{=StItems.ScenicName}}<i></i></h2>
                    </div>
                    {{??StItems.ScenicType == "food"}}
                    <div class="route-title title-view title-eatting">
                        <h2>美食：{{=StItems.ScenicName}}<i></i></h2>
                    </div>
                    {{?}}
                    <div class="detail-routes">{{=StItems.Description}}</div>
                    {{?StItems.ScenicImages&&StItems.ScenicImages.length}}
                    <div class="viewPic-box">
                        <ul>
                            {{~StItems.ScenicImages:itImgs:index2}}
                            {{?index2<3}}
                                <li><img src="{{=itImgs.replace('http:','')}}" alt="{{=StItems.ScenicName}}"></li>
                            {{?}}
                            {{~}}
                        </ul>
                    </div>
                    {{?}}
                </div>
                {{??StItems.Type==3}}
                <div class="route-hotel-box route-title">
                    <h2>{{=StItems.Description}}<i></i></h2>
                </div>
                {{?}}
                {{~}}
                {{?}}
            </li>
        {{~}}
        </ul>
    </div>
