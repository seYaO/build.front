            <div class="mapinfo-days clearfix">
                {{?it.PoiRoute.Routes && it.PoiRoute.Routes.length}}
                    {{~it.PoiRoute.Routes:poiMap:mapIndex}}
                        {{?mapIndex < 30}}
                            {{
                                var desPlace = '';
                                var latlng = [];
                                var nowTitle = '';
                                if(poiMap.Destinations && poiMap.Destinations.length){
                                    if(poiMap.Destinations.length == 1){
                                        desPlace = poiMap.Destinations[0];
                                    }else{
                                        for(var i=0; i<poiMap.Destinations.length; i++){
                                            desPlace += poiMap.Destinations[i] + '、';
                                        }
                                        desPlace = desPlace.substring(0,desPlace.length -1);
                                    }
                                }
                                if(desPlace.length > 5){
                                    nowTitle = desPlace.substring(0,5) + '...';
                                }else{
                                    nowTitle = desPlace;
                                }
                                
                                if(poiMap.Pois && poiMap.Pois.length){
                                    for(var i=0; i<poiMap.Pois.length; i++){
                                        if(poiMap.Pois[i].PoiType != 0){
                                            var lat = poiMap.Pois[i].Lat;
                                            var lng = poiMap.Pois[i].Lon;
                                            var title = '';
                                            var title = poiMap.Pois[i].Name + poiMap.Pois[i].EngilshName;
                                            latlng.push({
                                                lng: lng,
                                                lat: lat,
                                                title: title
                                            });
                                        }
                                    }
                                }
                            }}
                            <div class="mapinfo-item J_click_days {{?mapIndex == 0}}active{{?}}" data-latlng = "{{=encodeURIComponent(JSON.stringify(latlng))}}" title="{{=desPlace}}"><span>D{{=mapIndex + 1}}</span>{{=nowTitle}}</div>
                        {{?}}
                    {{~}}
                {{?}}
            </div>
