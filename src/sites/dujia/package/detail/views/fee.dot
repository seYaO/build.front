{{?it.IncludeFees&&it.IncludeFees.length}}
<div class="title-costContain">费用包含</div>
<div class="costCotain-box clearfix">
{{~it.IncludeFees:includeItems:index}}
    {{?includeItems.Type=="flight"}}
    <div class="cost contain-fly">
        <div class="cost-item">
            <i></i>
            <span>机票</span>
        </div>
        <p>{{=includeItems.Description}}</p>
    </div>
    {{?}}
    {{?includeItems.Type=="tax"}}
    <div class="cost contain-shui">
        <div class="cost-item">
            <i></i>
            <span>税费</span>
        </div>
        <p>{{=includeItems.Description}}</p>
    </div>
    {{?}}
    {{?includeItems.Type=="localtransport"}}
    <div class="cost contain-car">
        <div class="cost-item">
            <i></i>
            <span>当地交通</span>
        </div>
        <p>{{=includeItems.Description}}</p>
    </div>
    {{?}}
    {{?includeItems.Type=="hotel"}}
    <div class="cost contain-hotel">
        <div class="cost-item">
            <i></i>
            <span>酒店</span>
        </div>
        <p>{{=includeItems.Description}}</p>
    </div>
    {{?}}
    {{?includeItems.Type=="meal"}}
    <div class="cost contain-food">
        <div class="cost-item">
            <i></i>
            <span>用餐</span>
        </div>
        <p>{{=includeItems.Description}}</p>
    </div>
    {{?}}
    {{?includeItems.Type=="ticket"}}
    <div class="cost contain-menpiao">
        <div class="cost-item">
            <i></i>
            <span>门票</span>
        </div>
        <p>{{=includeItems.Description}}</p>
    </div>
    {{?}}
    {{?includeItems.Type=="guide"}}
    <div class="cost contain-daofu">
        <div class="cost-item">
            <i></i>
            <span>导服费</span>
        </div>
        <p>{{=includeItems.Description}}</p>
    </div>
    {{?}}
    {{?includeItems.Type=="visa"}}
    <div class="cost contain-visa">
        <div class="cost-item">
            <i></i>
            <span>签证</span>
        </div>
        <p>{{=includeItems.Description}}</p>
    </div>
    {{?}}
    {{?includeItems.Type=="singleroompoor"}}
    <div class="cost contain-danfang">
        <div class="cost-item">
            <i></i>
            <span>单房差</span>
        </div>
        <p>{{=includeItems.Description}}</p>
    </div>
    {{?}}
    {{?includeItems.Type=="driver"}}
    <div class="cost contain-siji">
        <div class="cost-item">
            <i></i>
            <span>司机</span>
        </div>
        <p>{{=includeItems.Description}}</p>
    </div>
    {{?}}
    {{?includeItems.Type=="personalcost"}}
    <div class="cost contain-geren">
        <div class="cost-item">
            <i></i>
            <span>个人消费</span>
        </div>
        <p>{{=includeItems.Description}}</p>
    </div>
    {{?}}
    {{?includeItems.Type=="baggage"}}
    <div class="cost contain-xingli">
        <div class="cost-item">
            <i></i>
            <span>行李费</span>
        </div>
        <p>{{=includeItems.Description}}</p>
    </div>
    {{?}}
    {{?includeItems.Type=="insurance"}}
    <div class="cost contain-baoxian">
        <div class="cost-item">
            <i></i>
            <span>保险费</span>
        </div>
        <p>{{=includeItems.Description}}</p>
    </div>
    {{?}}
    {{?includeItems.Type=="other"}}
    <div class="cost contain-other">
        <div class="cost-item">
            <i></i>
            <span>其他</span>
        </div>
        <p>{{=includeItems.Description}}</p>
    </div>
    {{?}}
{{~}}
</div>
{{?}}
