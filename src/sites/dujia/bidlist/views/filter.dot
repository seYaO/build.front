<div class="crumlist">
{{var conditionList = it.Conditions;}}
{{?(it.BaseData.PageType == 1 || it.BaseData.PageType == 3) && conditionList.LineClassification && conditionList.LineClassification.length}}
<div class="rowbox" data-key="lineQuality" data-list="1" data-openurl="false"
        data-multiselect="false" attr-name="线路分类" trace="lineQuality">
    <h4>线路分类 :</h4>
    <div class="rowlist J_lineType">
        {{~conditionList.LineClassification:lineQuality}}
        <a class="J_Tips" data-content="{{=lineQuality.Title}}" href="javascript:void(0);" data-value="{{=lineQuality.Value}}" date-loaded="true" trace="{{=lineQuality.Value}}">{{=lineQuality.Name}}</a>
        {{~}}
    </div>
</div>
{{?}}
{{?conditionList.LinePlay && conditionList.LinePlay.length}}
<div class="rowbox" data-key="specialNickId" data-list="2" data-openurl="false" data-multiselect="false" trace="specialNickId" attr-name="线路玩法">
    <h4>线路玩法 :</h4>
    <div class="rowlist J_linePlay">
        {{~conditionList.LinePlay:wanFa}}
        <a href="javascript:;" data-value="{{=wanFa.Value}}" trace="{{=wanFa.Value}}" title="{{=wanFa.Name}}">{{=wanFa.Name}}</a>
        {{~}}
        <div class="btns none">
            <span attr-data="" class="J_Trace submit" trace="filter-submit">确定</span>
            <span class="J_Trace cancel" trace="filter-cancel">取消</span>
        </div>
    </div>
    <div class="operationbox">
        <a class="multibtn" href="javascript:void(0);" title="" trace="multibtn">多选</a>
        <span class="J_Trace more-btn" trace="more">更多</span>
    </div>
</div>
{{?}}
{{?conditionList.TravelDay && conditionList.TravelDay.length}}
<div class="rowbox" data-key="days" data-list="3" data-openurl="false" data-multiselect="false" trace="days" attr-name="游玩天数" >
    <h4>游玩天数 :</h4>
    <div class="rowlist J_lineDays">
        {{~conditionList.TravelDay:dayList}}
        <a href="javascript:;" title="{{=dayList.Name}}" trace="{{=dayList.Value}}" data-value="{{=dayList.Value}}">{{=dayList.Name}}</a>
        {{~}}
        <div class="btns none">
            <span attr-data="" class="submit">确定</span>
            <span class="cancel">取消</span>
        </div>
    </div>
    <div class="operationbox">
        <a class="multibtn" href="javascript:void(0);" title="">多选</a>
        <span class="more-btn">更多</span>
    </div>
</div>
{{?}}
{{?conditionList.TravelDate && conditionList.TravelDate.length}}
<div class="rowbox" data-key="pm|clractTitleId|minVGDate|maxVGDate" data-list="4" data-openurl="false" data-multiselect="false" attr-name="出游日期" trace="chuyou-date">
    <h4>出游日期 :</h4>
    <div id="datelist" class="rowlist J_lineStartDate">
        <div class="daylist">
            {{~conditionList.TravelDate:travelDate}}
            <a href="javascript:;" title="{{=travelDate.Title}}" data-key="pm" data-value="{{=travelDate.Value}}">{{=travelDate.Name}}</a>
            {{~}}
            {{~conditionList.Season:clractTitle}}
            <a href="javascript:;" title="{{=clractTitle.Title}}" data-key="clractTitleId" data-value="{{=clractTitle.Value}}">{{=clractTitle.Name}}</a>
            {{~}}
        </div>
        <div class="calbox">
            <div class="mcalbox">
                <input id="startTime" placeholder="最早出发" readonly="true" class="mCal" data-key="MinVGDate" attr-timeb="" attr-timee="" type="text" />
            </div>
            <em class="cal_em">&nbsp;&nbsp;~&nbsp;&nbsp;</em>
            <div class="mcalbox2">
                <input id="endTime" placeholder="最晚出发" readonly="true" class="mCal" data-key="MaxVGDate" attr-timeb="" type="text" />
            </div>
            <em class="cal_btn">确定</em>
        </div>
    </div>
    <div class="operationbox">
        <span class="more-btn">更多</span>
    </div>
</div>
{{?}}
{{?conditionList.StartCity && conditionList.StartCity.length}}
{{var dataKey = "", href = "";}}
<div class="rowbox" data-key="{{=dataKey}}" data-list="5" data-openurl="false" data-multiselect="false" attr-name="出发城市" trace="chuyou-city">
    <h4>出发城市 :</h4>
    <div class="rowlist J_lineCity spm-deliver">
        {{~conditionList.StartCity:srcName}}
            {{var labelId = "",
                propType = "",
                dataValue = "";}}
            {{dataValue = srcName.Value;}}
            {{?it.BaseData.PageType == 0}}
                {{propType = "lvyou";}}
            {{??it.BaseData.PageType == 1}}
                {{labelId = it.BaseData.GroupLabelId;}}
                {{propType = "gentuan";}}
            {{??it.BaseData.PageType == 3}}
                {{labelId = it.BaseData.FreeLabelId;}}
                {{propType = "zizhu";}}
            {{??it.BaseData.PageType == 10}}
                {{propType = "local";}}
            {{?}}
            {{href = "/dujia/" + it.BaseData.DestPinYin + labelId + "-" + propType + "-jingjia/f" + srcName.Value + "/#cityanchor";}}
            <a href="{{=href}}" citypy="" data-spmed="true" data-value="{{=dataValue}}" title="{{=srcName.Name}}">{{=srcName.Name}}</a>
        {{~}}
        <div class="btns none">
            <span attr-data="" class="submit">确定</span>
            <span class="cancel">取消</span>
        </div>
    </div>
    <div class="operationbox">
        <span class="more-btn">更多</span>
    </div>
</div>
{{?}}
{{?conditionList.LineFeature &&conditionList.LineFeature.length}}
<div class="rowbox" data-key="rcImgTitleId" data-list="6" data-openurl="false" data-multiselect="false" attr-name="线路特色" trace="line-spec">
    <h4>线路特色 :</h4>
    <div class="rowlist J_lineFeature">
        {{~conditionList.LineFeature:serviceFeature}}
        <a href="javascript:;" title="{{=serviceFeature.Name}}" data-value="{{=serviceFeature.Value}}">{{=serviceFeature.Name}}</a>
        {{~}}
        <div class="btns none">
            <span attr-data="" class="submit">确定</span>
            <span class="cancel">取消</span>
        </div>
    </div>
    <div class="operationbox">
        <span class="more-btn">更多</span>
    </div>
</div>
{{?}}
{{var showText = "";}}
<div class="showboxs none">
    {{?it.BaseData.PageType != 3 && conditionList.TravelSpot && conditionList.TravelSpot.length}}
    {{?showText}}
        {{showText = showText + "、";}}
    {{?}}
    {{showText = showText + "行程景点";}}
    <div class="rowbox" data-key="csSId" data-list="7" data-openurl="false" data-multiselect="false" attr-name="行程景点" trace="line-scenery">
        <h4>行程景点 :</h4>
        <div class="rowlist J_lineScenery">
            {{~conditionList.TravelSpot:scenery}}
            <a href="javascript:;" data-value="{{=scenery.Value}}" title="{{=scenery.Name}}">{{=scenery.Name}}</a>
            {{~}}
        </div>
        <div class="operationbox">
            <span class="more-btn">更多</span>
        </div>
    </div>
    {{?}}
    {{?it.BaseData.PageType == 0 || it.BaseData.PageType == 3}}
    {{?conditionList.HotelLevel && conditionList.HotelLevel.length}}
    {{?showText}}
        {{showText = showText + "、";}}
    {{?}}
    {{showText = showText + "酒店类型";}}
    <div class="rowbox" data-key="hotelGradeId" data-list="6" data-openurl="false" data-multiselect="false" attr-name="酒店类型">
        <h4>酒店类型 :</h4>
        <div class="rowlist J_hotelType">
            {{~conditionList.HotelLevel:hotelList}}
            <a href="javascript:;" title="{{=hotelList.Name}}" data-value="{{=hotelList.Value}}">{{=hotelList.Name}}</a>
            {{~}}
            <div class="btns none"> <span attr-data="" class="submit">确定</span> <span class="cancel">取消</span> </div>
        </div>
        <div class="operationbox">
            <span class="more-btn">更多</span>
        </div>
    </div>
    {{?}}
    {{?conditionList.FlightType && conditionList.FlightType.length}}
    {{?showText}}
        {{showText = showText + "、";}}
    {{?}}
    {{showText = showText + "航班类型";}}
    <div class="rowbox" data-key="flightTypeId" data-list="7" data-openurl="false" data-multiselect="false" attr-name="航班类型">
        <h4>航班类型 :</h4>
        <div class="rowlist J_flightType">
            {{~conditionList.FlightType:flightList}}
            <a href="javascript:;" title="{{=flightList.Name}}" data-value="{{=flightList.Value}}">{{=flightList.Name}}</a>
            {{~}}
            <div class="btns none"> <span attr-data="" class="submit">确定</span> <span class="cancel">取消</span> </div>
        </div>
        <div class="operationbox">
            <span class="more-btn">更多</span>
        </div>
    </div>
    {{?}}
    {{?conditionList.GoTime && conditionList.GoTime.length}}
    {{?showText}}
        {{showText = showText + "、";}}
    {{?}}
    {{showText = showText + "出发时间";}}
    <div class="rowbox" data-key="goTime" data-list="8" data-openurl="false" data-multiselect="false" attr-name="出发时间">
        <h4>出发时间 :</h4>
        <div class="rowlist J_goTime">
            {{~conditionList.GoTime:startTime}}
                <a href="javascript:;" data-value="{{=startTime.Value}}">{{=startTime.Name}}</a>
            {{~}}
            <div class="btns none"> <span attr-data="" class="submit">确定</span> <span class="cancel">取消</span> </div>
        </div>
        <div class="operationbox">
            <span class="more-btn">更多</span>
        </div>
    </div>
    {{?}}
    
    {{?conditionList.BackTime && conditionList.BackTime.length}}
    {{?showText}}
        {{showText = showText + "、";}}
    {{?}}
    {{showText = showText + "返回时间";}}
    <div class="rowbox" data-key="backTime" data-list="9" data-openurl="false" data-multiselect="false" attr-name="返回时间">
        <h4>返回时间 :</h4>
        <div class="rowlist J_backTime">
            {{~conditionList.BackTime:endTime}}
                <a href="javascript:;" data-value="{{=endTime.Value}}">{{=endTime.Name}}</a>
            {{~}}
            <div class="btns none"> <span attr-data="" class="submit">确定</span> <span class="cancel">取消</span> </div>
        </div>
        <div class="operationbox">
            <span class="more-btn">更多</span>
        </div>
    </div>
    {{?}}
    {{?}}
</div>
{{?showText}}
<span class="showBtn">高级选项<b>（{{=showText}}）</b><i>展开</i></span>
{{?}}
<div class="sort-row">
    <div class="sort-left">
        <ul class="sort-ul">
            <li class="cur">
                <a href="javascript:;" title="综合排序">综合排序</a> <i>|</i>
            </li>
            <li>
                <a class="sortsty " href="javascript:;" title="按照30天销量排序">销量</a> <i>|</i>
            </li>
            <li>
                <a class="sortsty " href="javascript:;" title="按照线路满意度从高到低排序">点评</a> <i>|</i>
            </li>
            <li>
                <a class="sortsty" href="javascript:;" title="价格">价格</a>
            </li>
        </ul>
        <div class="pricebox">
            <div id="priceinner">
                <div class="inputbox">
                    <input class="min" type="text" maxlength="6" placeholder="¥" value=""/>
                    <em>~</em>
                    <input class="max" type="text" maxlength="6" placeholder="¥" value=""/>
                </div>
                <div class="price-bot none">
                    <span>清除价格</span>
                    <a href="javascript:;" title="确定">确定</a>
                </div>
            </div>
        </div>
    </div>
    {{?conditionList.TcSpecialLine && conditionList.TcSpecialLine.Value == 1}}
    <div class="extra">
        <span data-content="<div class='extra-all'><div class='extra-left'><img class='extra-lyline' src=http://pic3.40017.cn/line/admin/2015/10/27/18/0n16g0.png /></div> <div class='extra-right'><p class='p1'>什么是同程专线?</p><p class='p2'>同程旅游联合境内外优秀供应商为用户甄选提供更专业更高品质的产品。</p></div></div>" class="tc-line J_TcLine J_Tips"><i>&nbsp;</i></span>
    </div>
    {{?}}
</div>
{{?conditionList.Service && conditionList.Service.length}}
<div class="filter-row J_lineService">
    {{~conditionList.Service:lineFeature}}
    <span class="pitchon" data-key="serviceCharId" data-value="{{=lineFeature.Value}}">{{=lineFeature.Name}}</span>
    {{~}}
</div>
{{?}}
</div>