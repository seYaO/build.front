            {{?}}
        </td>
    </tr>
    {{?}}
    {{~}}
    {{?}}
    {{?product.Insurance && product.Insurance.Insurance && product.Insurance.Insurance.length >0}}
    {{var insurance = product.Insurance.Insurance;}}
    {{var accident = [],cancel = [];}}
    {{for(var i = 0;i< insurance.length;i++){ }}
    {{?insurance[i].FunctionType===1 }}
    {{accident.push(insurance[i]);}}
    {{??}}
    {{cancel.push(insurance[i]);}}
    {{?}}
    {{ } }}
    {{?}}
    {{?accident && accident.length>0}}
        <tr>
            <td class="td-firstChild pro-type">
                <p>意外险</p>
            </td>
            <td colspan="5" class="resource-items J_showToTop" style="padding-bottom: 0;">
            <span class="none" style="position: absolute;bottom: 12px;left: 0;width: 900px;color: #ff7800;">*旅游意外险能够给您的出行带来更多安全保障，所以同程旅游强烈建议您购买旅游意外险。如您放弃购买，则行程中的风险和损失将由您自行承担。</span>
            {{?accident.length >1}}
            <div class="resource-more ui-resource-hide">
                <a href="javascript:void(0)" class="label-click-color label-click-size J_moreResource">更多意外险</a>
            </div>
            {{?}}
                {{for(var i = 0;i
                < accident.length;i++){ }} {{?i===0 }} <div class="resource-box J_resource-box">

