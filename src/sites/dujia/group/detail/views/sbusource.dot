                                {{=cancel[i].detail}}{{=cancel[i].CustomerInsReferUrl}}
                            </div>
                        </div>
                        {{?}}
                    </div>
                    {{ } }}
            </td>
        </tr>
        {{?}}
        {{?product.WanLe && product.WanLe.length >0}}
        {{~product.WanLe:wanle}}
