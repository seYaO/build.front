                            <div style="padding: 18px 18px;">
                                {{=accident[i].detail}}{{=accident[i].CustomerInsReferUrl}}
                            </div>
                        </div>
                        {{?}}
                    </div>
                    {{ } }}
            </td>
        </tr>
        {{?}} {{?cancel && cancel.length >0}}
