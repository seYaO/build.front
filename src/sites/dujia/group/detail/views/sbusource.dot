                                {{??}}
                                <a href="javascript:void(0)" class="resource-radio J_resource-radio J_resource-safeRadio  resource-radio-normal"></a>
                                {{?}}
                            </div>
                            <div class="resource-item th_col_06 ">

                            </div>
                        </div>
                        {{?accident[i].detail && accident[i].detail !== ''}}
