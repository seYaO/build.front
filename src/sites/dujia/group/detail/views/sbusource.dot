                                <div class="resource-more ui-resource-hide">
                                    <a href="javascript:void(0)" class="label-click-color label-click-size J_moreResource">更多{{=wanle.Category}}</a>
                                </div>
                            </div>
                            {{?}}
                        </div>

                        <div class="resource-info none">
                        </div>

                    </div>
                    {{ } }}
            </td>
        </tr>
