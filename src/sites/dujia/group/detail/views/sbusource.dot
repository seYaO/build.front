<div class="resource-module clearfix">
<div class="module-name">
    <div class="icon-contain">
        <span></span>
    </div>
    <p class="module-title">可选资源</p>
</div>
<div class="resource-contains">
<table class="resource-container ">
<tbody>
    <tr class="resource-container-th">
        <th class="th_col_01">产品类型</th>
        <th class="th_col_02">产品名称</th>
        <th class="th_col_03">同程价</th>
        <th class="th_col_04">使用时间</th>
        <th class="th_col_05" style="left:30px">份数</th>
        <th class="th_col_06"></th>
    </tr>
    {{var hidLineDate = $('#hidLineDate').val();}}
    {{?it.product}} {{var product = it.product;}}
    {{?it.paramData && it.paramData.length !=0}}
    {{~it.paramData:paramData}}
    {{?paramData.PriceType != 1 && paramData.PriceType != 8}}
    <tr>
        <td class="td-firstChild pro-type">
            <p title='{{=paramData.PriceDes}}'>{{=paramData.PriceDes}}</p>
        </td>
        <td colspan="5" class="resource-items">
            <div class="resource-box J_resource-box clearfix">
                <div class="J_resourceBox" trackkit="{{?paramData.PriceName == '单房差'}}single{{?}}">
                    <div class="resource-item th_col_02">
                        <span class="label-click-size J_resource-box-name">{{=paramData.PriceName}}</span>
                    </div>
                    <div class="resource-item th_col_03" >
                        <span class="price-common-color price-common-size J_orderItem" data-time="{{=hidLineDate}}" data-type="{{=paramData.PriceType}}" data-priceId="{{=paramData.PriceId}}" data-resourceid='' data-productid='' data-usingDays=''>￥<b class="price-large-size J_resource-box-price"
