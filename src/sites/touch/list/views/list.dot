{{var lineList = it.lineList;}}
{{?lineList && lineList.length && lineList[0].line && lineList[0].line.length}}
{{~lineList[0].line:value:listIndex}}
    {{?value}}
        {{var tracePos;}}
        {{var traceLineType;}}
        {{var traceLineId;}}
        {{var traceType;}}
        {{var _isHot = value.scoreTopRecd>0 && it.Page==1;}}
        {{?value.prop == "1"}}
            {{tracePos = 1;}}
            {{traceLineType = 1;}}
        {{??}}
            {{tracePos = 3;}}
            {{traceLineType = 3;}}
        {{?}}
        {{traceLineId = value.id;}}
        {{?value.isTCSpecialLine == 1}}
            {{traceType = 1;}}
        {{??value.isExtraordinary == 1}}
            {{traceType = 2;}}
        {{??}}
            {{traceType = 0;}}
        {{?}}
        <div class="auction" data-isHot="{{?_isHot && (listIndex < 3)}}1{{?}}" data-trace = "{{=tracePos + '^' + traceLineType + '^'+'^' + traceLineId + '^' + traceType}}">
            <div class="main-pic">
                <a class="img-area" href="/dujia/tours/{{=value.id}}.html" data-trace = "{{=tracePos + '^' + traceLineType + '^'+'^' + traceLineId + '^' + traceType}}">
                    <img alt="{{=value.title}}" data-img="{{=value.absImgURL.replace('http:','')}}"/>
                    {{?value.portCity}}<span class="time">{{=value.portCity}}出发</span>{{?}}
                    {{?value.prop == "1"}}<span class="icon_gentuan">跟团游</span>{{?}}
                    {{?value.prop == "3"}}<span class="icon_free">自由行</span>{{?}}
                </a>
                <div class="describe">
                    <div class="title">
                    <b><{{=value.title}}><em>{{=value.subTitle}}</em></b>
                    <div class="infos">
                        {{?value.isTCSpecialLine == 1}}<label class="label label--blue">同程专线</label>{{?}}
                        {{?value.isExtraordinary == 1}}<label class="label label--yellow">非凡系列</label>{{?}}
                        {{?value.isStrict == 1 && value.strictContent}}
                            <label class="label" style="color:#ff6d4f; border:1px solid #ff6d4f;">同程严选</label>
                        {{?}}
                        {{?value.isSuper == 1 && value.superContent}}
                            <label class="label" style="color:#ff6d4f; border:1px solid #ff6d4f;">VIP超值购</label>
                        {{?}}
                        {{?value.isBlh == 1 && value.blhContent}}
                            <label class="label" style="color:#ff6d4f; border:1px solid #ff6d4f;">积分抵扣</label>
                        {{?}}
                        {{var disList = value.disList;}}
                        {{?disList && disList.length && disList[0].discount && disList[0].discount.length}}
                            {{~disList[0].discount:rule}}
                                {{?rule.name != '积分抵扣' && rule.name != 'VIP超值购' && rule.name != '同程严选'}}
                                    <label class="label" style="color: {{=rule.iconColor}}; border: 1px solid {{=rule.iconColor}}">{{=rule.name}}</label>
                                {{?}}
                            {{~}}
                        {{?}}
                        {{?value.lineQualityName!=""}}
                            <label class="label label--green">{{=value.lineQualityName}}</label>
                        {{?}}
                    </div>
                    </div>
                    {{?value.satDeg == "0.0"}}
                        <div class="remark">
                            <span class="remark-label">
                                新品
                            </span>
                        </div>
                    {{??}}
                        {{var index=value.satDeg.indexOf(".");}}
                        {{var count=value.satDeg.substring(0,index);}}
                        <div class="remark">
                            <span class="remark-label">{{=count}}%满意</label>
                            <em>|</em>
                            <span class="remark-label">{{=value.cmtCount}}条点评</label>
                        </div>
                    {{?}}
                    <div class="new-price">
                        <p>
                            <span class="new-tcPrice"><em>￥</em><b>{{=value.tcPrice}}</b>起</span>
                            {{?value.IsShowRedBag===true}}
                                <span class="red_pri">
                                    <!--<span class="red-packet">最高</span>
                                    <span class="J_redPrice">¥{{=value.CutNum}}</span>
                                    <em>减</em>-->
                                        红包最高可减{{=value.CutNum}}元
                                </span>
                            {{?}}
                            {{?value.strictAmount > 0}}
                            <span class="allTravel">
                                <span class="allTravel-name">严选</span>
                                <span class="allTravel-price">抵{{=value.strictAmount}}元/人 返3%</span>
                            </span>
                            {{??value.superAmount > 0}}
                            <span class="allTravel">
                                <span class="allTravel-name">VIP超值购</span>
                                <span class="allTravel-price">抵{{=value.superAmount}}元/人</span>
                            </span>
                            {{??value.blhAmount > 0}}
                            <span class="allTravel">
                                <span class="allTravel-name">积分抵扣</span>
                                <span class="allTravel-price">抵{{=value.blhAmount}}元/人</span>
                            </span>
                            {{?}}
                        </p>
                    </div>
                </div>
            </div>
            {{?_isHot && (listIndex < 3)}}<span class="retui"></span>{{?}}
        </div>
    {{?}}
{{~}}
{{?}}